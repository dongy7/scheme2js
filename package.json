{
  "name": "scheme2js",
  "version": "1.1.3",
  "description": "Scheme to JavaScript transpiler",
  "repository": "https://github.com/dongy7/scheme2js",
  "author": "Dong Yeop Lee <dong@dongyeop.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "s2j": "dist/cli.js"
  },
  "dependencies": {
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/meow": "^4.0.1",
    "@types/node": "^10.3.2",
    "jest": "^23.1.0",
    "prettier": "^1.13.5",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/*.+(ts|tsx|js)"
    ]
  }
}
